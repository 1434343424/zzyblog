                                    <input type="password" name="password" placeholder="Password"></br>

                                </div>

                                <div>
                                    <label for="remember"><input type="checkbox" id="remember">记住我</label>
                                </div>
                                <div class="form-group">
                                    <p>没有注册？<a href="/reg">注册</a> | <a href="#">忘记密码？</a></p>
                                </div>
                                <input type="submit" value="登录">
                            </form>
                            <!-- END Sign In Form -->
                        </div>

                        <!--<div class="col-md-5">-->
                            <!--<div><img src="imgs/10.jpg" alt="" style="height:400px;width:450px;margin-left:40px;"></div>  &lt;!&ndash; google map &ndash;&gt;-->
                        <!--</div>-->
                    </div>
            </div>

        </div> <!-- end contact form outer row with centered text-->

        <div class="row"> <!-- outer row -->
            <div class="col-md-10 col-md-offset-1"> <!-- col 10 with offset 1 to centered -->
                <div class="row"> <!-- nested row -->

                    <!-- contact detail using col 4 -->
                    <div class="col-md-4">
                        <div class="contact-detail">
                            <i class="fa fa-map-marker"></i>
                            <h4>中国</h4> <!-- address -->
                        </div>
                    </div>
                    <!-- contact detail using col 4 -->
                    <div class="col-md-4">
                        <div class="contact-detail">
                            <i class="fa fa-envelope-o"></i>
                            <h4>1434343424@qq.com</h4><!-- email add -->
                        </div>
                    </div>

                    <!-- contact detail using col 4 -->
                    <div class="col-md-4">
                        <div class="contact-detail">
                            <i class="fa fa-phone"></i>
                            <h4>15238907980</h4> <!-- phone no. -->
                        </div>
                    </div>

                </div> <!-- end nested row -->
            </div> <!-- end col 10 with offset 1 to centered -->
        </div><!-- end outer row -->

