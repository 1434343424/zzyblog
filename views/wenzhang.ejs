            <div class="row"> <!-- row -->
                <div class="col-md-7 col-md-offset-1"> <!-- Left Blogrol col 8 -->

                    <!-----------------------------文章-->
                    <% posts.forEach(function (post, index) { %>
                    <div class="post-wrap"> <!-- Post Wrapper -->
                        <div class="media post"> <!-- post wrap -->
                            <div class="media-left">
                                <a href="#"> <!-- link to your post single page -->
                                    <img class="media-object"  src="imgs/<%= Math.floor(Math.random() * 8) %>.jpg" alt="..." id="wenzhangimg"> <!-- Your Post Image -->
                                </a>
                            </div>
                            <div class="media-body">
                                <p class="small"><a>标签：
                                        <% post.tags.forEach(function (tag, index) { %>
                                        <% if (tag) { %>
                                        <a class="tag" href="/tags"><%= tag %></a>
                                        <% } %>
                                        <% }) %></a>日期：<%= post.time.minute %></p>
                                <a href="/u/<%= post.name %>/<%= post.time.minute %>/<%= post.title %>" style="color:red">
                                    <h5 class="media-heading"><strong>标题：<%= post.title %></strong></h5>
                                </a>
                                <div id="p1"><%- post.post %>
                                </div>
                            </div>
                        </div><!-- end post wrap -->

                        <div class="post-meta"> <!-- Meta details -->
                            <ul class="list-inline metas pull-left"> <!-- post metas -->
                                <li>作者： <b href="#"><%= post.name %></b></li> <!-- meta author -->
                                <li>日期：<b href="#"><%= post.time.minute %></b></li> <!-- meta comments -->
                                <li><a href="#">read more!</a></li> <!-- read more link -->
                            </ul>
                            <ul class="list-inline meta-detail pull-right"> <!-- user meta interaction -->
                                <li><a href="#"><i class="fa fa-heart"></i></a><%= Math.floor(Math.random() * 100) %></li> <!-- like button -->
                                <li><i class="fa fa-eye"></i><%= Math.floor(Math.random() * 10000) %></li> <!-- no. of views -->
                            </ul>
                        </div><!-- end Meta details -->
                    </div><!-- end Post Wrapper -->
                    <% }) %>


                    <div class="text-left"> <!-- Blogrol Pagination -->
                        <nav>
                            <div id="page">



                            </div>
